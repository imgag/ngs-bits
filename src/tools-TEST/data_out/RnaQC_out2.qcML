    <qualityParameter ID="qp0001" name="housekeeping genes read percentage" description="Percentage of reads that could be mapped to the exon region of housekeeping genes." value="9.92" cvRef="QC" accession="QC:2000100"/>
    <qualityParameter ID="qp0002" name="housekeeping genes read depth" description="Average sequencing depth in exon region of housekeeping genes." value="37.07" cvRef="QC" accession="QC:2000101"/>
    <qualityParameter ID="qp0003" name="housekeeping genes 10x percentage" description="Percentage of the exon region of housekeeping genes that is covered at least 10-fold." value="100.00" cvRef="QC" accession="QC:2000102"/>
    <qualityParameter ID="qp0004" name="housekeeping genes 20x percentage" description="Percentage of the exon region of housekeeping genes that is covered at least 20-fold." value="48.05" cvRef="QC" accession="QC:2000103"/>
    <qualityParameter ID="qp0005" name="housekeeping genes 30x percentage" description="Percentage of the exon region of housekeeping genes that is covered at least 30-fold." value="43.28" cvRef="QC" accession="QC:2000104"/>
    <qualityParameter ID="qp0006" name="housekeeping genes 50x percentage" description="Percentage of the exon region of housekeeping genes that is covered at least 50-fold." value="32.36" cvRef="QC" accession="QC:2000105"/>
    <qualityParameter ID="qp0007" name="housekeeping genes 100x percentage" description="Percentage of the exon region of housekeeping genes that is covered at least 100-fold." value="2.09" cvRef="QC" accession="QC:2000106"/>
